    "luxon": "^2",
    "password-meter": "^3.9.3",
    "process": "^0.11.10",
    "stream-browserify": "^3.0.0",
    "svelte-hash-router": "^1.0.1",
    "tweetnacl": "^1.0.3",
    "url-pattern": "^1.0.3"
  },
  "devDependencies": {
    "@tsconfig/svelte": "^1.0.10",
    "@types/bs58": "^4.0.1",
    "@types/commonmark": "^0.27.4",
    "@types/luxon": "^2",
    "esbuild": "0.18.8",
    "esbuild-copy-plugin": "^1.0.0",
    "esbuild-plugin-inline-worker": "^0.1.1",
    "esbuild-svelte": "*",
    "protoc-gen-ts": "^0.8.2",
